{
  "name": "@effect-ts-app/boilerplate-types",
  "version": "0.5.0",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@effect-ts-app/core": "^2.4.8",
    "@effect-ts-app/schema": "^2.3.18",
    "@effect-ts-app/boilerplate-prelude": "^2.4.8",
    "@effect-ts/core": "^0.60.5",
    "@effect/io": "^0.0.65",
    "@fp-ts/core": "^0.0.11",
    "@fp-ts/data": "^0.0.39",
    "date-fns": "^2.29.3",
    "uuid": "^9.0.0",
    "validator": "^13.7.0"
  },
  "peerDependencies": {
    "date-fns": "^2.29.3"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    "./ext": {
      "import": {
        "types": "./dist/ext.d.ts",
        "default": "./dist/ext.js"
      },
      "require": {
        "types": "./dist/ext.d.ts",
        "default": "./_cjs/ext.cjs"
      }
    },
    "./Blog": {
      "import": {
        "types": "./dist/Blog.d.ts",
        "default": "./dist/Blog.js"
      },
      "require": {
        "types": "./dist/Blog.d.ts",
        "default": "./_cjs/Blog.cjs"
      }
    },
    "./User": {
      "import": {
        "types": "./dist/User.d.ts",
        "default": "./dist/User.js"
      },
      "require": {
        "types": "./dist/User.d.ts",
        "default": "./_cjs/User.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./_cjs/*.cjs"
      }
    }
  },
  "devDependencies": {
    "@types/validator": "^13.7.10",
    "@types/faker": "5.5.9",
    "@types/uuid": "^9.0.0",
    "faker": "^5.5.3",
    "fast-check": "^3.6.0"
  },
  "scripts": {
    "build": "pnpm clean-dist && NODE_OPTIONS=--max-old-space-size=6144 tsc --build",
    "build2:cjs": "rm -rf _cjs && babel dist --config-file ../../.babel.cjs.json --out-dir _cjs --out-file-extension .cjs --source-maps",
    "watch": "pnpm build --watch",
    "watch2": "pnpm clean-dist && NODE_OPTIONS=--max-old-space-size=6144 tsc -w",
    "clean": "rm -rf dist && rm -rf _cjs",
    "clean-dist": "sh ../../scripts/clean-dist.sh",
    "circular": "pnpm circular:src && pnpm circular:dist",
    "circular:src": "madge --circular --ts-config ./tsconfig.json --extensions ts ./_src",
    "circular:dist": "madge --circular --extensions js ./dist",
    "compile": "NODE_OPTIONS=--max-old-space-size=6144 tsc --noEmit",
    "lint": "NODE_OPTIONS=--max-old-space-size=6144 ESLINT_TS=1 eslint ./_src",
    "lint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx .",
    "autofix": "pnpm lint --fix",
    "test": "vitest",
    "test:run": "pnpm run test run --passWithNoTests",
    "testsuite": "pnpm lint && pnpm circular && pnpm run test:run",
    "ncu": "ncu"
  },
  "gitHead": "bd8e27eea3eff97db8739d577d67e7336c078d28",
  "sideEffects": [
    "./dist/faker.js",
    "./faker",
    "./src/faker",
    "./dist/faker",
    "./dist/src/faker.js",
    "./dist/faker.js"
  ]
}